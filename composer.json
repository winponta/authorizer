{
    "name": "winponta/authorizer",
    "description": "Auth package dependent of zizaco/entrust",
    "keywords": [
        "winponta",
        "authorizer",
        "auth"
    ],
    "homepage": "https://github.com/winponta/authorizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Ademir Mazer Jr (Nuno Mazer)",
            "email": "ademir.mazer.jr@gmail.com",
            "homepage": "http://ademir.winponta.com.br"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Winponta\\Authorizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Winponta\\Authorizer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
